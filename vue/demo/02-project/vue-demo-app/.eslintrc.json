{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/recommended",
    "@vue/eslint-config-prettier"
  ],
  "plugins": ["vue"],
  "rules": {
    "vue/require-default-prop": "off",
    "vue/v-bind-style": "off",
    "vue/v-on-style": "off",
    "vue/no-v-html": "off",
    "prettier/prettier": "off",
    "generator-star-spacing": "off",
    "no-unused-vars": "off",
    "no-console": "off"
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "globals": {
    "location": true,
    "document": true,
    "window": true,
    "GLOBAL": true
  }
}
